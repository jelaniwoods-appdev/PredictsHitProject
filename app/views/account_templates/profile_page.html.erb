<% if @account_info.id == current_user.id %>

<!-- Add edit form -->
  <div class="col-md-6 offset-md-3">
    <div class="jumbotron">
      <h1 class="center_title">My Profile</h1>
      <br>
      <div>Username: <%= @account_info.username %></div>
      <br>
      <div>Email Address: <%= @account_info.email %></div>
      <br>
      <div>Current Profile Picture:</div>
      <br>
      <div><img src="<%= @account_info.prof_pic %>" height = 200></div>
      <br>
      <div class="center_title"><a class="btn btn-secondary btn-block" href="/account/manage/<%= @account_info.id %>">Manage Account</a></div>
      <br>
    </div>
  </div>


<% else %>

  <div class="col-md-6 offset-md-3">
    <div class="jumbotron">
      <h1 class="center_title">User Profile</h1>
      <br>
      <div>Username: <%= @account_info.username %></div>
      <br>
      <div>Current Profile Picture:</div>
      <br>
      <div><img src="<%= @account_info.prof_pic %>" height = 200></div>   
    </div>
  </div>

<% end %>